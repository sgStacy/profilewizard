    <fields>
        <fullName>APXT_CMQM__Launch__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(  ISPICKVAL( APXT_CMQM__Link_Type__c , &quot;PointMerge&quot;) , 

Hyperlink( &quot;https://www.appextremes.com/apps/Conga/PointMerge.aspx&quot;
&amp; &quot;?sessionId=&quot; &amp; $Api.Session_ID
&amp; &quot;&amp;serverUrl=&quot; &amp; $Api.Partner_Server_URL_80
&amp; &quot;&amp;id=&quot; &amp; Id
&amp; &quot;&amp;ReportId=&quot; &amp; SUBSTITUTE( APXT_CMQM__Report_Id__c , &quot;User.Id&quot;, $User.Id )
&amp; &quot;&amp;TemplateId=&quot; &amp; APXT_CMQM__Document_Template_Id__c
&amp; APXT_CMQM__Other_Parameters__c
, IMAGE( &quot;https://www.appextremes.com/apps/Conga/Images/Launch.png&quot;, &quot;Launch PointMerge&quot;) ) ,

Hyperlink( &quot;https://www.appextremes.com/apps/Conga/Welcome.aspx&quot;
&amp; &quot;?sessionId=&quot; &amp; $Api.Session_ID
&amp; &quot;&amp;serverUrl=&quot; &amp; $Api.Partner_Server_URL_80
&amp; &quot;&amp;ReportId=&quot; &amp;  SUBSTITUTE( APXT_CMQM__Report_Id__c , &quot;User.Id&quot;, $User.Id )
&amp; &quot;&amp;CampaignId=&quot; &amp;  APXT_CMQM__Campaign_Id__c 
&amp; &quot;&amp;QueryId=&quot; &amp;  SUBSTITUTE( APXT_CMQM__Query_Id__c , &quot;User.Id&quot;,  $User.Id )
&amp; &quot;&amp;QueryId1=&quot; &amp;   SUBSTITUTE( APXT_CMQM__Query_Id_1__c , &quot;User.Id&quot;,  $User.Id )
&amp; &quot;&amp;DocTemplateId=&quot; &amp;  APXT_CMQM__Document_Template_Id__c 
&amp; &quot;&amp;LabelTemplateId=&quot; &amp;  APXT_CMQM__Label_Template_Id__c 
&amp; &quot;&amp;EnvTemplateId=&quot; &amp;  APXT_CMQM__Envelope_Template_Id__c 
&amp; &quot;&amp;bpw=&quot; &amp;    IF( APXT_CMQM__Bypass_Wizard__c = true, &quot;1&quot;, &quot;&quot;)
&amp; &quot;&amp;DefaultPDF=&quot; &amp;  IF( APXT_CMQM__Default_to_PDF__c  = true, &quot;1&quot;, &quot;&quot;)
&amp; &quot;&amp;fp0=&quot; &amp;  IF( APXT_CMQM__Force_Output_as_PDF__c = true, &quot;1&quot;, &quot;&quot;)
&amp; &quot;&amp;lg0=&quot; &amp;  IF( APXT_CMQM__LogWordBody__c  = true, &quot;1&quot;, &quot;&quot;)
&amp; &quot;&amp;lg1=&quot; &amp;  APXT_CMQM__Activity_Subject__c 
&amp; &quot;&amp;lg2=&quot; &amp; TEXT( APXT_CMQM__Follow_up_Interval__c  )
&amp; &quot;&amp;lg4=&quot; &amp;  IF( APXT_CMQM__Automatic_Logging__c  = true, &quot;1&quot;, &quot;&quot;)
, IMAGE( &quot;https://www.appextremes.com/apps/Conga/Images/Launch.png&quot;, &quot;Launch Conga Merge&quot;) ) )</formula>
        <label>Launch</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
